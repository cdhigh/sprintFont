      [(<span class=3D"hljs-name">solder_paste_margin_ratio</span> RATIO)] =
                      <i class=3D"conum" data-value=3D"22"></i><b>(<span cl=
ass=3D"hljs-number">22</span>)</b>
      [(<span class=3D"hljs-name">clearance</span> CLEARANCE)]             =
                      <i class=3D"conum" data-value=3D"23"></i><b>(<span cl=
