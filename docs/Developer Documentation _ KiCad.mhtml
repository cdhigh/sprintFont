      [(<span class=3D"hljs-name">thermal_width</span> WIDTH)]             =
                      <i class=3D"conum" data-value=3D"25"></i><b>(<span cl=
ass=3D"hljs-number">25</span>)</b>
      [(<span class=3D"hljs-name">thermal_gap</span> DISTANCE)]            =
                      <i class=3D"conum" data-value=3D"26"></i><b>(<span cl=
ass=3D"hljs-number">26</span>)</b>
      [CUSTOM_PAD_OPTIONS]                                      <i class=3D=
"conum" data-value=3D"27"></i><b>(<span class=3D"hljs-number">27</span>)</b=
>
      [CUSTOM_PAD_PRIMITIVES]                                   <i class=3D=
"conum" data-value=3D"28"></i><b>(<span class=3D"hljs-number">28</span>)</b=
>
    )</code><span class=3D"copy-to-clipboard" title=3D"Copy to clipboard"><=
/span></pre>
